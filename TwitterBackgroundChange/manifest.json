{
    "name": "Twitter Background Change",
    "version": "1.0",
    "description": "Change background for twitter.com, Popop has a button to click which changes body background, Options helps you to choose background",
    "permissions": ["storage", "activeTab", "scripting"],
    "options_page": "options.html",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      },
      
    "manifest_version": 3
  }